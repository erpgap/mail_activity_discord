<!-- data/access_rights.xml -->
<odoo>
    <data noupdate="1">
        <!-- Access rules for Discord Bot -->
        <record id="access_discord_bot_user" model="ir.model.access">
            <field name="name">access_discord_bot_user</field>
            <field name="model_id" ref="model_discord_bot"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>
        <record id="access_res_config_settings_user" model="ir.model.access">
            <field name="name">access_res_config_settings_user</field>
            <field name="model_id" ref="base.model_res_config_settings"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="0"/>
        </record>
    </data>
</odoo>
