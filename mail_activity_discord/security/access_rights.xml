<odoo>
    <data noupdate="1">
        <!-- Access Rights for Discord Bot -->
        <record id="access_discord_bot" model="ir.model.access">
            <field name="name">discord.bot.access</field>
            <field name="model_id" ref="model_discord_bot"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>

        <!-- Access Rights for Mail Activity -->
        <record id="access_mail_activity_discord" model="ir.model.access">
            <field name="name">mail.activity.access</field>
            <field name="model_id" ref="mail.model_mail_activity"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>

        <!-- Access Rights for Mail Activity Type -->
        <record id="access_mail_activity_type" model="ir.model.access">
            <field name="name">mail.activity.type.access</field>
            <field name="model_id" ref="mail.model_mail_activity_type"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>

        <!-- Access Rights for Res Config Settings -->
        <record id="access_res_config_settings" model="ir.model.access">
            <field name="name">res.config.settings.access</field>
            <field name="model_id" ref="base.model_res_config_settings"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>

        <!-- Access Rights for Res Users -->
        <record id="access_res_users" model="ir.model.access">
            <field name="name">res.users.access</field>
            <field name="model_id" ref="base.model_res_users"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>
    </data>
</odoo>
